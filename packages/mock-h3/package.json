{
  "name": "vite-plugin-mock-h3",
  "type": "module",
  "version": "1.0.0",
  "description": "A mock server for Vite using H3",
  "author": "aibayanyu20",
  "keywords": [
    "vite",
    "mock",
    "h3"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./dist": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "vite"
  },
  "dependencies": {
    "bundle-require": "^5.1.0",
    "chokidar": "^3.6.0",
    "h3": "2.0.0-beta.1",
    "rou3": "^0.7.3",
    "tinyglobby": "^0.2.14"
  }
}
