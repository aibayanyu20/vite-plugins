{
  "name": "@mistjs/vite-plugin-px2viewport",
  "type": "module",
  "version": "0.1.0",
  "description": "this is a vite plugin for inline px to viewport",
  "author": "aibayanyu20 <aibayanyu@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/aibayanyu20/vite-plugins/#readme",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aibayanyu20/vite-plugins.git"
  },
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.js",
      "require": "./dist/vue.cjs",
      "default": "./dist/vue.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "play": "vite play",
    "prepublish": "pnpm build"
  },
  "peerDependencies": {
    "typescript": ">=5",
    "vite": ">=4"
  },
  "dependencies": {
    "@babel/generator": "^7.23.4",
    "@babel/parser": "^7.23.4",
    "@babel/traverse": "^7.23.4",
    "@babel/types": "^7.23.4",
    "magic-string-ast": "^0.6.2",
    "mlly": "^1.7.1",
    "postcss-px-to-viewport-8-plugin": "^1.2.5"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.7",
    "@types/babel__traverse": "^7.20.4"
  }
}
